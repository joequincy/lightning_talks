<div class="row">
  <div class="col l6 offset-l3 s10 offset-s1">
    <%= form_for @lightning_talk, url: admin_lightning_talks_path do |f| %>
      <%= f.label :name, "Name" %>
      <%= f.text_field :name %>
      <%= f.label :final_date, "Date of Finals" %>
      <%= f.date_field :final_date %>
      <%= f.submit "Create Lightning Talk!", class: "btn turing-btn"%>
    <% end %>
  </div>
</div>
