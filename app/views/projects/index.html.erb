<h1>Projects</h1>

<div class="unvoted">
  <h1>Unvoted</h1>
  <% @projects.check_votes.each do |project| %>
    <p><%= link_to project.name, new_project_vote_path(project) %></p>
  <% end %>
</div>

<div class="voted">
  <h1>Already Voted</h1>
  <% @projects.check_votes(current_user.id).each do |project| %>
    <p><%= project.name %></p>
  <% end %>
</div>
